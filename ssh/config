Include config.local

AddKeysToAgent yes

Host alex-desktop
    HostName alex-desktop
    User alex
    IdentityFile ~/.ssh/id_rsa
    ForwardAgent yes

Host RouterMain
    HostName RouterMain
    User root
    IdentityFile ~/.ssh/id_rsa

Host RouterBridge
    HostName RouterBridge
    User root
    IdentityFile ~/.ssh/id_rsa

Host Alexs-MBP
    HostName Alexs-MBP
    User alex
    IdentityFile ~/.ssh/id_rsa
    ForwardAgent yes

ControlMaster auto
ControlPath /tmp/ssh_control_%h_%p_%r
ControlPersist 2h

